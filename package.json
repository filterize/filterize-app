{
  "name": "Filterize",
  "author": "Pascal Held",
  "homepage": "http://filterize.net/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "watch": "ionic-app-scripts watch",
    "serve:before": "watch",
    "emulate:before": "build",
    "deploy:before": "build",
    "build:before": "build",
    "run:before": "build",
    "electron": "electron .",
    "electron dist": "electron .",
    "test": "grunt bump-only:prerelease && ionic serve",
    "browserify": "browserify -s main www/build/main.js | exorcist www/build/bundle.js.map > www/build/bundle.js",
    "minify": "uglifyjs www/build/main.js --in-source-map www/build/main.js.map --source-map www/build/main.min.js.map --screw-ie8 --compress warnings=false --mangle --output www/build/main.min.js",
    "start": "grunt bump && ionic build browser && grunt swPrecache:release && firebase deploy"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@angular/router": "3.2.1",
    "@ionic/storage": "1.1.7",
    "@ngrx/core": "^1.1.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/router-store": "^1.2.5",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.2",
    "angular2-jwt": "0.1.27",
    "ionic-angular": "2.0.0",
    "ionic-native": "2.2.13",
    "ionicons": "3.0.0",
    "ng2-translate": "^5.0.0",
    "pouchdb": "^6.1.0",
    "raven-js": "^3.9.1",
    "rxjs": "5.0.0-beta.12",
    "strftime": "^0.10.0",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "@ionic/app-scripts": "1.0.0",
    "browserify": "^13.3.0",
    "electron-packager": "^8.2.0",
    "electron-prebuilt": "^1.4.13",
    "exorcist": "^0.4.0",
    "grunt": "^1.0.1",
    "grunt-bump": "^0.8.0",
    "ionic-minify": "^2.0.10",
    "node-version-compare": "^1.0.1",
    "sw-precache": "^4.2.3",
    "typescript": "2.0.9",
    "uglify-js": "^2.7.5"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "ios",
    {
      "platform": "ios",
      "version": "",
      "locator": "ios"
    }
  ],
  "main": "electron.js",
  "description": "Filterize Console allows you to configure your filterize filter."
}
